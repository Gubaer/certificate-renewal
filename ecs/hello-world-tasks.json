{
  "family": "hello-world-task",
  "networkMode": "awsvpc",
  "cpu": "256",
  "memory": "512",
  "executionRoleArn": "arn:aws:iam::154819770423:role/ecsTaskExecutionRole",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "containerDefinitions": [
    {
      "name": "hello-world",
      "image": "154819770423.dkr.ecr.eu-central-1.amazonaws.com/hello-world",
      "entryPoint": [
        "bash"
      ],
      "command": [
        "/hello-world/hello-world.sh"
      ],
      "memory": 32
    }
  ]
}