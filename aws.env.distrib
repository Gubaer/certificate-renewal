#
# template AWS enviroment variables 
# - edit, then copy to aws.env
# - then activate with
#   $ source aws.env
#
#
# the AWS access key
export AWS_ACCESS_KEY_ID=
# the AWS secret acces key
export AWS_SECRET_ACCESS_KEY=

# the AWS default region, i.e. eu-central-1
export AWS_DEFAULT_REGION=eu-central-1
# the AWS region, i.e. eu-central-1
export AWS_REGION=eu-central-1